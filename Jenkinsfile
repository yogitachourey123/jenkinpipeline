pipeline {
    agent any
      stages {
        stage('Build') {
            steps {
                echo "hello world"
            }
        }
        stage('Run') {
            steps {
                echo "welcome"
            }
        }
         stage('Build') { 
            steps {
             echo"Building"   
            }
        }
        stage('Test') { 
            steps {
               echo"Testing" 
            }
        }
        stage('Deploy') { 
            steps {
              echo"Deploying"  
            }
        }
    }
}
